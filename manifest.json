{
  "name": "KeepKey Wallet",
  "description": "Complete cryptocurrency integration with Chrome.",
  "version": "0.8.13",
  "manifest_version": 2,
  "options_page": "src/keepkey.html#options",
  "browser_action": {
    "default_icon": "src/assets/icons/128x128.png",
    "default_title": "KeepKey Wallet",
    "default_popup": "src/popup.html"
  },
  "icons": {
    "16": "src/assets/icons/16x16.png",
    "48": "src/assets/icons/48x48.png",
    "128": "src/assets/icons/128x128.png"
  },
  "permissions": [
    "management"
  ],
  "background": {
    "scripts": [
      "src/background.js"
    ]
  }
}
